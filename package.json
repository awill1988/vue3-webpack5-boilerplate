    "import-sort-style-module-and-prefix": "^0.1.3",
    "is-windows": "^1.0.2",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "lint-staged": "^10.5.0",
    "mini-css-extract-plugin": "^1.3.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
