{
  "name": "vue3-webpack5-boilerplate",
  "version": "0.0.0",
  "license": "MIT",
  "main": "webpack.config.babel.js",
  "scripts": {
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "npm run lint:eslint \"src/\"",
    "lint:all:stylelint": "npm run lint:stylelint \"src/**/*.{vue,css}\"",
    "lint:all:prettier": "npm run lint:prettier \"**/*.{ts,json,css,vue}\"",
    "lint": "run-s lint:all:*",
    "clean:config": "rimraf webpack/**/*.js webpack/*.js webpack.config.babel.js",
    "compile:config": "tsc --project tsconfig.tooling.json",
    "start": "webpack serve --env mode=dev --env isDevServer --config webpack.config.babel.js --progress",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.babel.js",
    "postinstall": "npm run clean:config && npm run compile:config",
    "typecheck": "tsc --noEmit",
    "profile": "webpack --profile --json --config webpack.config.babel.js > ./build/profile.json && webpack-bundle-analyzer ./build/profile.json",
    "prestart": "npm run clean:config && npm run compile:config"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-proposal-throw-expressions": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@svgr/webpack": "^5.4.0",
    "@teamsupercell/typings-for-css-modules-loader": "^2.4.0",
    "@types/copy-webpack-plugin": "^6.3.0",
    "@types/is-windows": "^1.0.0",
    "@types/mini-css-extract-plugin": "^1.2.1",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@types/webpack-env": "^1.16.0",
    "@types/yargs": "^15.0.10",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
